<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">


        <shape android:shape="rectangle">
            <corners android:radius="8dp"></corners>
            <stroke
                android:width="2dp"
                android:color="@color/gri"></stroke>

            <solid android:color="@color/beyaz"></solid>
        </shape>
    </item>
</layer-list>